<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <referenceContainer name="content">
                <block ifconfig="pyxl_pixels/fb/enable" class="Pyxl\Pixels\Block\Page"
                       name="fb_page" template="Pyxl_Pixels::fb/page.phtml" after="-"/>
                <block ifconfig="pyxl_pixels/pinterest/enable" class="Pyxl\Pixels\Block\Page"
                       name="pinterest_page" template="Pyxl_Pixels::pinterest/page.phtml" after="-"/>
            </referenceContainer>
        </referenceContainer>
    </body>
</page>
